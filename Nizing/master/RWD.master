        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-61636827-1', 'auto');
        ga('send', 'pageview');

        $(document).ready(function () {
            navbarLinkToggle();
        });

        $(window).resize(function () {
            navbarLinkToggle();
        });

        function navbarLinkToggle() {
            if ($(window).width() < '768') {
                $('.nav-link.has-child').attr('data-toggle', 'dropdown');
            }
            else {
                $('.nav-link.has-child').removeAttr('data-toggle');
            }
        }
    </script>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
<body>
    <form id="form1" runat="server">
        <header class="container-fluid">
            <nav runat="server" id="navigation">
                <div class="navbar navbar-expand-md fixed-top nav-wrapper">
                    <a class="navbar-brand mr-0" href="/">
                        <img src="/images/logo/nizing.png" />
                    </a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#NavContent">
                        <span class="fas fa-bars"></span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-between" id="NavContent">
                        <div class="navContentList-wrapper">
                            <ul runat="server" id="divNavContentList"
                                class="navbar-nav">
                                <!--dynamic content BulidMenu-->
                            </ul>
                        </div>
                        <hr class="d-md-none" />

                        <div class="d-flex justify-content-md-between">
                            <div class="pinned-item">
                                <a data-toggle="modal" data-target="#gcse" title="搜尋網站">
                                    <div style="color: white">
                                        <i class="fas fa-search"></i>
                                    </div>
                                </a>
                            </div>
                            <div class="dropdown pinned-item">
                                <a data-toggle="dropdown" aria-expanded="false" title="變更語言"
                                    style="cursor: pointer;">
                                    <i class="fas fa-globe"></i>
                                </a>
                                <div class="dropdown-menu dropdown-menu-lg-right">
                                    <asp:LinkButton ID="zh" CssClass="dropdown-item" runat="server" OnClick="ChangeLanguage">中文</asp:LinkButton>
                                    <asp:LinkButton ID="en" CssClass="dropdown-item" runat="server" OnClick="ChangeLanguage">English</asp:LinkButton>
                                </div>
                            </div>
                            <div class="pinned-item">
                                <a href="/zh/portal" target="_blank" title="職員專區">
                                    <i class="fas fa-user"></i>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </nav>
        </header>
        <div class="body">
            <asp:ContentPlaceHolder ID="banner" runat="server">
            </asp:ContentPlaceHolder>
            <div class="breadcrumb">
                <div class="container">
                    <asp:SiteMapPath ID="NizingSiteMapPath" runat="server" SkipLinkText="" CssClass="sitemap-path">
                        <RootNodeTemplate>
                            <a href="/">
                                <img src="/images/home_black-48x48.png" class="img" />
                            </a>
                        </RootNodeTemplate>
                    </asp:SiteMapPath>
                </div>
            </div>
            <asp:ContentPlaceHolder ID="body" runat="server">
            </asp:ContentPlaceHolder>
        </div>
        <footer class="footer">
            <div class="container">
                <div id="divFooterMenuList" runat="server" class="row row-m-0 row-cols-1 row-cols-md-4 w-100">
                </div>
                <small class="disclaimer">©2021 Nizing Electric Wire & Cable Co., Ltd
                </small>
            </div>
        </footer>

        <!--Modals-->
        <div id="gcse" class="modal fade">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title"></h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                        </button>
                    </div>
                    <div class="modal-body">
                        <%--Google Search Engine Here--%>
                        <div class="gcse-searchbox-only">
                        </div>
                    </div>
                    <div class="modal-footer">
                    </div>
                </div>
            </div>
        </div>
    </form>
</body>
</html>
