    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.bundle.min.js" integrity="sha384-1CmrxMRARb6aLqgBO7yyAxTOQE2AKb9GfXnEo760AUcUmFx3ibVJJAzGytlQcNXd" crossorigin="anonymous"></script>
    <script src="https://kit.fontawesome.com/32e9a84b4a.js" crossorigin="anonymous"></script>
    <script src="https://unpkg.com/aos@2.3.1/dist/aos.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/ScrollMagic/2.0.8/ScrollMagic.min.js"></script>
    <script src="/Scripts/modernizr-custom.js"></script>
    <script src="/Scripts/utils.js"></script>


    <style>
        .body {
            padding-top: 56px;
        }

        .container-fluid {
            padding: 0;
        }

        /*navbar*/

        .nav-wrapper {
            background-color: #343a40;
            box-shadow: 0 3px 5px -2px #8f8f8f;
        }

        .navbar {
            background-color: #585858;
            opacity: 0.9;
        }


            .navbar .navbar-brand {
                padding: 0;
            }

                .navbar .navbar-brand img {
                    height: 40px !important;
                    width: 170px !important;
                }

            .navbar .nav-link {
                color: #ffffff;
            }

        .navbar-toggler {
            color: #585858;
            filter: invert(1) brightness(1.5);
        }

        .fa-search, .dropdown-item {
            cursor: pointer;
        }

        nav#navigation i {
            color: #585858;
            filter: invert(1) brightness(1.5);
        }

        nav#navigation hr {
            border-color: white;
        }

        .pinned-item {
            display: inline-block;
            padding-right: 0.75rem;
        }

        /*breakcrumb*/
        .breadcrumb {
            background-color: #ffffff;
            font-size: 0.75rem;
            margin-bottom: 0;
        }

            .breadcrumb .container {
                padding: 0;
            }

            .breadcrumb img {
                width: 20px;
            }

        /*footer*/
        .footer {
            background-color: #585858;
            color: #f7f7f7;
            padding-top: 1rem;
        }

            .footer .card {
                margin-bottom: 24px;
            }

                .footer .card .title-wrapper {
                    padding-bottom: 0.5rem;
                    margin-left: 0;
                    margin-right: 0;
                    border-bottom: solid 1px #cccccc;
                }

            .footer .card-title {
                color: #f7f7f7;
                font-size: 1.2rem;
                font-weight: bold;
                margin-bottom: 0;
            }

            .footer .card-body {
                padding: 0px;
                font-size: 1rem;
            }

                .footer .card-body .card-link.img {
                    font-size: 2rem;
                }

                .footer .card-body li {
                    padding: 0px;
                }

                .footer .card-body a {
                    color: #f7f7f7;
                }

                    .footer .card-body a:hover {
                        cursor: pointer;
                        color: #ffffff;
                        transition: font-size linear 0.1s;
                    }

            .footer .card, .footer .list-group-item {
                text-align: left;
                background-color: inherit;
                border: 0px;
            }

            .footer .card-body .list-group-item {
                padding-top: 0.5rem;
            }

            .footer .disclaimer {
                display: flex;
                justify-content: center;
                font-size: 0.7rem;
                width: 100%;
                padding-bottom: 0.25rem;
            }


        @media all and (min-width: 768px) {

            .navContentList-wrapper {
                width: 100%;
                display: flex;
                justify-content: center;
            }

            .nav-wrapper {
                min-width: 885px;
            }

            #divNavContentList {
                justify-content: space-around;
                width: 100%;
                max-width: 900px;
            }

            .navbar .nav-item .dropdown-menu {
                display: none;
            }

            .navbar .nav-item:hover .dropdown-menu {
                display: block;
            }

            .navbar .nav-item .dropdown-menu {
                margin-top: 0;
            }

            /*breadcrumb*/
            .breadcrumb .container {
                padding-left: 15px;
                padding-right: 15px;
            }

            .footer {
                padding-left: 0px;
            }

                .footer .card {
                }

                    .footer .card .title-wrapper {
                        margin-left: 2rem;
                        margin-right: 2rem;
                    }

                .footer .card-title {
                    margin: 0;
                    padding-bottom: 10px;
                }

                .footer .card, .footer .list-group-item {
                    text-align: left;
                }

                .footer .card-body .list-group-item {
                    padding-top: 0.5rem;
                    padding-left: 2rem;
                }
        }
    </style>
    <script type="text/javascript">      
</script>
    <script type="text/javascript">
        (function () {
            var cx = '010054550380540391242:bbpyczxrrro';
            var gcse = document.createElement('script');
            gcse.type = 'text/javascript';
            gcse.async = true;
            gcse.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') +
